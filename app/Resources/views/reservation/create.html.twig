
{% if error is not defined  %}
{% if not form.vars.valid %}
<ul>
    
    {% for child in form.children %}
        
        {%for error in child.vars.errors%}
            <li>{{error.message}}</li>
        {%endfor%}
    {%endfor%}
</ul>
{%endif%}

{{ form_start(form, {'method': 'post', 'action': path('create_reservation',{productId: id }) }) }}

    <div id="resa">
             
    {{ form_rest(form) }}
	</div>
    
{{ form_end(form) }}


{% else %}
	<h1>{{ error }}</h1>
{% endif %}



